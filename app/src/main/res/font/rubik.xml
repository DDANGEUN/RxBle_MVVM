<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute,ResourceCycle">

    <!--light-->
    <font
        android:font="@font/rubik_light"
        android:fontStyle="normal"
        android:fontWeight="300"
        app:font="@font/rubik_light"
        app:fontStyle="normal"
        app:fontWeight="300" />

    <!--regular-->
    <font
        android:font="@font/rubik_regular"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/rubik_regular"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <!--medium-->
    <font
        android:font="@font/rubik_medium"
        android:fontStyle="normal"
        android:fontWeight="1000"
        app:font="@font/rubik_medium"
        app:fontStyle="normal"
        app:fontWeight="1000" />

    <!-- italic -->
    <font
        android:font="@font/rubik_lightitalic"
        android:fontStyle="italic"
        android:fontWeight="300"
        app:font="@font/rubik_lightitalic"
        app:fontStyle="italic"
        app:fontWeight="300" />
    <font
        android:font="@font/rubik_italic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/rubik_italic"
        app:fontStyle="italic"
        app:fontWeight="400" />
    <font
        android:font="@font/rubik_mediumitalic"
        android:fontStyle="italic"
        android:fontWeight="1000"
        app:font="@font/rubik_mediumitalic"
        app:fontStyle="italic"
        app:fontWeight="1000" />




</font-family>